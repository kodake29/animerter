<h1>Admins::Users#index</h1>
<p>Find me in app/views/admins/users/index.html.erb</p>
<table class="table table-striped">
<tr>
<th>会員ID</th>
<th>会員名</th>
<th>会員メールアドレス</th>
<th>ステータス</th>
</tr>
<% @users.each do |user| %>
<tr>
<td><%= user.id %></td>
<td><%=link_to user.name, admins_user_path(user) %></td>
<td><%= user.email%></td>
<td><% if user.withdrawal_status == 0 %>
<p>有効会員</p>
<%else%>
<P>退会済み</P>
<% end %>
</td>
</tr>
<% end %>
</table>
